{
  "name": "lazycrypto-cli",
  "version": "0.1.3",
  "description": "A simple CLI app to view cryptocurrency indicators",
  "type": "module",
  "main": "index.js",
  "bin": {
    "lazycrypto": "./dist/index.js"
  },
  "scripts": {
    "build": "babel . --out-dir dist --extensions .js,.jsx --ignore '**/node_modules/**,**/dist/**'  && cp package.json dist/",
    "start": "npm run build && node dist/index.js",
    "dev": "babel src --out-dir dist --extensions .js,.jsx --watch"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^17.2.1",
    "indicatorts": "^2.2.2",
    "ink": "^4.4.1",
    "ink-select-input": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "react": "^18.2.0",
    "update-notifier": "^7.3.1"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "crypto",
    "cryptocurrency",
    "cli",
    "terminal",
    "indicators"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "@babel/preset-react": "^7.27.1"
  }
}
